[run]
source = crypto_j_trader
omit = 
    tests/*
    */tests/*
    */site-packages/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    raise AssertionError

[html]
directory = coverage_html_report